{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "outputDirectory": "public",
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(css|js|images)/(.*)$",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/build/(.*)$",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
